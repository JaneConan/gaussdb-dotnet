<Project>
  <Import Project="../Directory.Build.props" />

  <PropertyGroup>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <IsPackable>true</IsPackable>
    <Authors>HuaweiCloud</Authors>
    <Company>Huawei Cloud</Company>
    <NamespacePrefix>HuaweiCloud.</NamespacePrefix>
    <PackagePrefix>HuaweiCloud.Driver.</PackagePrefix>
    <AssemblyName>$(PackagePrefix)$(MSBuildProjectName)</AssemblyName>
    <RootNamespace>$(NamespacePrefix)$(MSBuildProjectName)</RootNamespace>
    <PackageId>HuaweiCloud.Driver.$(MSBuildProjectName)</PackageId>
    <PackageTags>gaussdb;openguass;ado;ado.net;database;sql</PackageTags>
  </PropertyGroup>

</Project>
